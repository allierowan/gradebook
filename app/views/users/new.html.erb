<h1>Create Login</h1>
<%= form_for @user do |f| %>
  <div class="form-group">
    <%= f.label :email %>
    <%= f.email_field :email, class:"form-control" %>
  </div>

  <div class="form-group">
    <%= f.label :password %>
    <%= f.password_field :password, class:"form-control" %>
  </div>

  <%= f.submit "Save", class:"btn btn-primary" %>

<% end %>
